<div class="row">
    <div class="col col-span-1"><h1>All Categories</h1></div>
</div>

<div class="row">
    <div class="col col-span-1">
        <div class="container">
            <table>
                #foreach($category in $categories)
                <tr>
                    <td><a href="/categories/$category">$category.getName()</a></td>
                </tr>
                #end
            </table>
        </div>
    </div>
</div>